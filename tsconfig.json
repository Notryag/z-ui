{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["unplugin-vue-macros/macros-global"],
  },
  "include": ["packages"],
  // "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
